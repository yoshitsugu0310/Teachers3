<% if @feed_items.present? && current_user.status == "student" %>

    <% @feed_items.each do |match_teacher| %>
    <ol class="teachers">
    <%= link_to match_teacher[0], teacher_path(match_teacher[0]) %>
    <%= match_teacher[1] %>
    </ol>
    <% end %>

    <% elsif @feed_items.any? && current_user.status == "teacher" %>

      <% @feed_items.each do |s| %>
      <ol class="students">
      <%= link_to s.user.name, student_path(s.user_id) %>


    </ol>
    <% end %>




<% end %>
